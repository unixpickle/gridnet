{
    "compilerOptions": {
        "module": "commonjs",
        "types": [
            "@webgpu/types"
        ],
        "lib": [
            "es2019",
            "dom",
        ],
        "target": "es6",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "outDir": "visualize/js",
    },
    "include": [
        "visualize/ts/*.ts",
    ],
}